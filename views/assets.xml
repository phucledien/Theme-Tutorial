<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <data>
    <template id="assets_frontend" name="website_assets" inherit_id="website.assets_frontend" priority="100">                                                      
       <xpath expr="link[last()]" position="after">
        <!-- Google Fonts -->
        <link href='http://fonts.googleapis.com/css?family=Titillium+Web:400,200,300,700,600' rel='stylesheet' type='text/css'/>
        <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:400,700,300' rel='stylesheet' type='text/css'/>
        <link href='http://fonts.googleapis.com/css?family=Raleway:400,100' rel='stylesheet' type='text/css'/>
        
        <!-- Bootstrap -->
        <link rel="stylesheet" href="/theme_tutorial/static/src/css/bootstrap.min.css"/>
        
        <!-- Font Awesome -->
        <link rel="stylesheet" href="/theme_tutorial/static/src/css/font-awesome.min.css"/>
        
        <!-- Custom CSS -->
        <link rel="stylesheet" href="/theme_tutorial/static/src/css/owl.carousel.css"/>
        <link rel="stylesheet" href="/theme_tutorial/static/src/css/style.css"/>
        <link rel="stylesheet" href="/theme_tutorial/static/src/css/responsive.css"/>

        
       </xpath>

        <xpath expr="script[last()]" position="after">
          <script type="text/javascript" src="/theme_tutorial/static/src/js/jquery.min.js" />
          <script type="text/javascript" src="/theme_tutorial/static/src/js/bootstrap.min.js" />
          <script type="text/javascript" src="/theme_tutorial/static/src/js/owl.carousel.min.js" />
          <script type="text/javascript" src="/theme_tutorial/static/src/js/jquery.sticky.js" />
          <script type="text/javascript" src="/theme_tutorial/static/src/js/jquery.easing.1.3.min.js" />
          <script type="text/javascript" src="/theme_tutorial/static/src/js/main.js" />
        </xpath>
    </template>
 
  </data>
</odoo>